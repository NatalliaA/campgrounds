<!DOCTYPE html>
<html>
    <head>
        <title>YelpCamp</title>
        <!-- Old Bootstrap V 3.3 -->
        <link rel = "stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
        <link rel="stylesheet" href="/stylesheets/landing.css">
        <!-- modernizr for old browsers' compatibility -->
        <script type="text/javascript" async
        src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.js"></script>

    </head>
    <body>
       
        <!-- Alert message -->
        <div class="container">
            <% if(error && error.length>0){ %>
                <div class= "alert alert-danger" role='alert'>
                    <%= error %>
                </div>
            <% } %>
            <% if(success && success.length>0) { %>
                <div class= "alert alert-success" role='alert'>
                    <%= success %>
                </div>
            <% } %>
        </div>

<div id="landing-header">
<h1 id="welcome">Welcome to YelpCamp!</h1>
<a href="/campgrounds" class='btn btn-lg btn-default'>View All Campgrounds</a>
</div>

